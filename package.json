{
  "name": "electron-demo",
  "version": "1.0.4",
  "description": "bla bla",
  "main": "electro-main.js",
  "author": "Sophatel",
  "license": "MIT",
  "repository": "git@github.com:FienexSistorm/electro-publisher.git",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "start:electron": "ng build --base-href ./ && electron .",
    "package-win": "electron-packager ./dist --overwrite  --platform=win32 --version-string.ProductName=\"Demo Sample Application\"",
    "dir": "electron-builder --dir",
    "pack": "elctron-builder --dir",
    "electro-publish": "electron-builder -p always",
    "dist": "electron-builder"
  },
  "private": false,
  "dependencies": {
    "@angular/animations": "~12.2.0",
    "@angular/common": "~12.2.0",
    "@angular/compiler": "~12.2.0",
    "@angular/core": "~12.2.0",
    "@angular/forms": "~12.2.0",
    "@angular/platform-browser": "~12.2.0",
    "@angular/platform-browser-dynamic": "~12.2.0",
    "@angular/router": "~12.2.0",
    "electron-log": "^4.4.6",
    "electron-updater": "^4.6.5",
    "rxjs": "~6.6.0",
    "tslib": "^2.3.0",
    "uaup-js": "0.0.22",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.2.9",
    "@angular/cli": "~12.2.9",
    "@angular/compiler-cli": "~12.2.0",
    "@types/jasmine": "~3.8.0",
    "@types/node": "^12.11.1",
    "electron": "^18.0.1",
    "electron-builder": "^22.14.13",
    "electron-packager": "^15.4.0",
    "jasmine-core": "~3.8.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "typescript": "~4.3.5"
  },
  "build": {
    "appId": "com.sophatel.winpharm",
    "productName": "Electron Demo",
    "buildVersion": "1.0.4",
    "target": "nsis",
    "win": {
      "target": "nsis"
    },
    "directories": {
      "output": "build"
    },
    "nsis": {
      "allowToChangeInstallationDirectory": true,
      "oneClick": false
    }
  }
}
